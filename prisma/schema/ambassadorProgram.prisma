model AmbassadorProgram {
  id                  String            @id @default(uuid())
  ambassadorRequestUserId       String
  requestStatus       AmbassadorStatus  @default(PENDING)

  motorspotName       String
  contactName         String
  email               String
  country             String

  instagramProfile    String?
  tiktokProfile       String?
  youTubeChanel       String?

  totalFollower       Int
  mainCar             String?
  whyDoYouWant        String
  releventExperience  String?
  profilePhoto        String?

  ambassadorRequestUser        User              @relation(fields: [ambassadorRequestUserId ], references: [id])

  createdAt           DateTime          @default(now())
  updatedAt           DateTime          @updatedAt
}