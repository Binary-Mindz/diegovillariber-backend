model OfficialPartner {
  id            String                       @id @default(uuid()) @db.Uuid
  requestUserId String                       @db.Uuid
  requestStatus OfficialPartnerRequestStatus @default(PENDING)

  brandLogo                 String?
  brandName                 String
  contactName               String
  contactEmail              String
  brandDescription          String?
  websiteUrl                String?
  industry                  String?
  country                   String?
  companyRegistrationNumber String?

  requestUser User @relation(fields: [requestUserId], references: [id])

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
