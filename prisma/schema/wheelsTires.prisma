model WheelsTires {
  id                String          @id @default(uuid()) @db.Uuid
  advancedCarDataId String          @unique @db.Uuid
  advancedCarData   AdvancedCarData @relation(fields: [advancedCarDataId], references: [id])

  wheelAlignmentFront Float?
  wheelAlignmentRear  Float?
  frontToe            Float?
  rearToe             Float?
  frontCaster         Float?
  alignmentNotes      String?
}
